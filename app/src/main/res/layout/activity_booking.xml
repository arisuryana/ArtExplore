<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BookingActivity">

    <TextView
        android:id="@+id/no_telp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/email_pembeli"
        android:layout_marginTop="8dp"
        android:layout_toRightOf="@id/no_telp1"
        android:layout_marginLeft="50dp"
        android:text="no_telp"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/no_telp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/email_pembeli"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="10dp"
        android:text="No. Telpon"
        android:textStyle="bold"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/jumlah_tiket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="22dp"
        android:text="jumlah_tiket"
        android:layout_toRightOf="@id/jumlah_tiket1"
        android:textSize="18sp"
       android:layout_below="@id/no_telp" />

    <TextView
        android:id="@+id/jumlah_tiket1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="10dp"
        android:text="Jumlah Ticket"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_below="@id/no_telp" />

    <TextView
        android:id="@+id/total_harga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp"
        android:text="total_harga"
        android:layout_toRightOf="@id/total_harga1"
        android:textSize="18sp"
        android:layout_below="@id/jumlah_tiket" />

    <TextView
        android:id="@+id/total_harga1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="8dp"
        android:text="Total Harga"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_below="@id/jumlah_tiket" />

    <TextView
        android:id="@+id/tempat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="8dp"
        android:text="tempat"
        android:layout_toRightOf="@id/tempat1"
        android:textSize="18sp"
        android:layout_below="@id/tanggal" />

    <TextView
        android:id="@+id/tempat1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        android:text="Tempat"
        android:textSize="18sp"
        android:layout_below="@id/tanggal" />

    <TextView
        android:id="@+id/tanggal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="71dp"
        android:text="tanggal"
        android:layout_toRightOf="@id/tanggal1"
        android:textSize="18sp"
        android:layout_below="@id/total_harga"/>

    <TextView
        android:id="@+id/tanggal1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="10dp"
        android:text="Tanggal"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_below="@id/total_harga"/>

    <TextView
        android:id="@+id/nama_pembeli"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@id/nama_pembeli1"
        android:layout_marginLeft="20dp"
        android:text="nama_pembeli"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/nama_pembeli1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Nama Pembeli"
        android:textStyle="bold"
        android:textSize="18sp"
        android:layout_marginLeft="10dp"/>

    <TextView
        android:id="@+id/email_pembeli"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/email_pembeli1"
        android:layout_below="@id/nama_pembeli"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="8dp"
        android:text="email pembeli"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/email_pembeli1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/nama_pembeli"
        android:textStyle="bold"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="8dp"
        android:text="Email Pembeli"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/jenis"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/no_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:gravity="center"
        android:layout_marginTop="320dp"
        android:text="Tidak ada tiket dibooking" />

    <Button
        android:id="@+id/clear_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tempat"
        android:layout_marginLeft="135dp"
        android:layout_marginTop="76dp"
        android:gravity="center"
        android:text="Sudah ditonton" />


    <Button
        android:id="@+id/home_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/clear_button"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="163dp"
        android:gravity="center"
        android:text="Home" />
</RelativeLayout>
